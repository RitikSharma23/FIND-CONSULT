<%- contentFor('breadcrumb') %>

<div class="row">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <h4 class="mb-sm-0">Add Business</h4>

            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">Add Business</a></li>
                    <li class="breadcrumb-item active">Step 2</li>
                </ol>
            </div>

        </div>
    </div>
</div>

<%- contentFor('body') %>

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div id="progrss-wizard" class="twitter-bs-wizard">
                    <ul class="twitter-bs-wizard-nav nav-justified">
                        <li class="nav-item">
                            <a href="#progress-seller-details" class="nav-link" >
                                <span class="step-number bg-light text-primary">01</span>
                                <span class="step-title">Basic Details</span>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#progress-bank-detail" class="nav-link"  >
                                <span class="step-number  bg-light text-primary">02</span>
                                <span class="step-title">Business Images</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#progress-company-document" class="nav-link"  >
                                <span class="step-number bg-light text-primary">03</span>
                                <span class="step-title">Business Credentials</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#progress-confirm-detail" class="nav-link"  >
                                <span class="step-number bg-light text-primary">04</span>
                                <span class="step-title">Business Documents</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#progress-confirm-detail" class="nav-link"  >
                                <span class="step-number bg-primary text-white">05</span>
                                <span class="step-title">Business Documents</span>
                            </a>
                        </li>
                    </ul>
                    <div  class="progress mt-4 mb-6">
                        <div class="bg-success progress-bar-striped progress-bar-animated" style="width: 100%;"></div>
                    </div>
            </div>
        </div>
    </div>
</div>

<div class="row" id="owner Details">
  

    <div class="col">
<center><button type="button" class="btn btn-primary waves-effect waves-light mb-4" data-bs-toggle="modal" data-bs-target=".bs-example-modal-sm">Add More Business Owner</button></center>

        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Incorporation Certificate</h4>
                    <p class="card-title-desc">Please Upload Incorporation Certificate In PDF format</p>
                    <div class="input-group">
                        <input type="file" class="form-control" id="incorporationCertificate">
                    </div>
                </div>
            </div>
        </div>
    
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Udyog Adhar</h4>
                    <p class="card-title-desc">Please Upload Udyog Adhar In PDF format</p>
                    <div class="input-group">
                        <input type="file" class="form-control" id="udyogAdhar">
                    </div>
                </div>
            </div>
        </div>
        
    </div>

    <% users.forEach(function(user) { %>
      <div class="col-lg-6">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Company Owner Details Details <%= user.created_by !=0 ? '(Default)':'' %></h4>
                <div>
                    <div class="mb-4">
                        <input class="form-control" id="youtube" type="text" value="Name : <%= user.name %>" disabled>
                    </div>
                    <div class="mb-4">
                        <input class="form-control" id="facebook" type="text" value="Email : <%= user.email %>" disabled>
                    </div>
                    <div class="mb-4">
                        <input class="form-control" id="instagram" type="text" value="Phone : <%= user.phone %>" disabled>
                    </div>
                    <div class="mb-4">
                        <input class="form-control" id="google" type="text" value="Aadhar : <%= user.aadhar %>" disabled>
                    </div>
                    <div class="mb-4">
                        <input class="form-control" id="google" type="text" value="PAN : <%= user.pan %>" disabled>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <% }) %>

</div>





<div class="col-sm-6 col-md-4 col-xl-3">
    <!--  Modal content for the above example -->
    <div class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title mt-0" id="mySmallModalLabel">Add Owner</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="col-lg-12">
                        <div class="mb-3">
                            <label class="form-label" for="progress-basicpill-lastname-input">Phone Number</label>
                            <input type="number" class="form-control" id="newPhone">
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="mb-3">
                            <label class="form-label" for="progress-basicpill-lastname-input">Email ID</label>
                            <input type="email" class="form-control" id="newEmail">
                        </div>
                    </div>


                <div class="modal-footer">
                    <button type="button" class="btn btn-primary waves-effect waves-light" id="addBusiness">Add Owner</button>
                    <button type="button" class="btn btn-light waves-effect" data-bs-dismiss="modal">Close</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
</div>






